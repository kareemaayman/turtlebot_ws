<launch>
  <!-- Include Gazebo empty world launch -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch" />

  <!-- Robot Description using xacro -->
  <param name="robot_description" command="$(find xacro)/xacro '$(find miarobot_description)/urdf/model.urdf.xacro'" />

  <!-- Spawn the robot in Gazebo -->
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" output="screen"
        args="-param robot_description -urdf -model shato_robot" />
</launch>
